{
  "name": "ngx-bb-emojione-picker",
  "version": "0.2.2",
  "license": "MIT",
  "main": "index.js",
  "module": "index.js",
  "scripts": {
    "********************** CLEAR **********************": "",
    "clear:js": "find src -type f \\( -iname '*.js' -o -iname '*.js.map' \\) -delete && rm -f index.js index.js.map",
    "clear:dts": "find src -type f -iname '*.d.ts' -delete && rm -f index.d.ts",
    "clear:css": "find src -type f \\( -iname '*.css' -o -iname '*.css.map' \\) -delete",
    "clear:test": "find test -type f \\( -iname '*.js' -o -iname '*.js.map' -o -iname '*.d.ts' \\) -delete",
    "clear:metadata": "find src -type f -iname '*.metadata.json' -delete && rm -f index.metadata.json",
    "clear:bundle": "rm -rf bundles/*",
    "clear:tsc": "npm run clear:js && npm run clear:dts",
    "clear:ngc": "npm run clear:tsc && npm run clear:metadata",
    "clear": "npm run clear:ngc && npm run clear:css && npm run clear:bundle && npm run clear:test",
    "clear:full": "npm run clear && rm -rf node_modules",
    "********************** TYPESCRIPT **********************": "",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "ngc": "ngc -p tsconfig-build.json",
    "********************** BUILD *********************": "",
    "build-css": "node-sass --include-path assets assets/style.scss   assets/style.css",
    "********************** TEST **********************": "",
    "********************** ROLLUP **********************": "",
    "rollup": "rollup -c rollup.config.js && npm run rollup:banner",
    "rollup:min": "rollup -c rollup-min.config.js && npm run rollup:min:banner",
    "rollup:banner": "cat license-banner.txt > bundles/ngx-bb-emojione-picker.umd.js && cat bundles/ngx-bb-emojione-picker.umd.tmp >> bundles/ngx-bb-emojione-picker.umd.js && rm bundles/ngx-bb-emojione-picker.umd.tmp",
    "rollup:min:banner": "cat license-banner.txt > bundles/ngx-bb-emojione-picker.umd.min.js && cat bundles/ngx-bb-emojione-picker.umd.min.tmp >> bundles/ngx-bb-emojione-picker.umd.min.js && rm bundles/ngx-bb-emojione-picker.umd.min.tmp",
    "********************** BUNDLE **********************": "",
    "bundle": "npm run clear && npm test && npm run build-css && npm run ngc && npm run rollup && npm run rollup:min"
  },
  "dependencies": {
    "@angular/animations": "^4.1.0",
    "@angular/common": "^4.1.0",
    "@angular/compiler": "^4.1.0",
    "@angular/core": "^4.1.0",
    "@angular/forms": "^4.1.0",
    "@angular/http": "^4.1.0",
    "@angular/material": "^2.0.0-beta.3",
    "@angular/platform-browser": "^4.1.0",
    "@angular/platform-browser-dynamic": "^4.1.0",
    "@angular/router": "^4.1.0",
    "core-js": "^2.4.1",
    "emojione": "^3.0.0",
    "hammerjs": "^2.0.8",
    "rxjs": "^5.1.0",
    "underscore": "^1.8.3",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/compiler-cli": "4.1.0",
    "@angular/tsc-wrapped": "4.1.0",
    "@types/jasmine": "2.5.47",
    "concurrently": "3.4.0",
    "jasmine-core": "2.5.2",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-jasmine": "1.1.0",
    "node-sass": "^4.5.2",
    "reflect-metadata": "0.1.10",
    "rollup": "0.41.6",
    "rollup-plugin-uglify": "1.0.1",
    "systemjs": "0.20.11",
    "typescript": "2.2.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
